{
  "name": "prefab-tools",
  "version": "0.6.0",
  "description": "Toolkit to analyze, trim, repopulate and manipulate your 7 Days To Die map",
  "author": "SuperSmack.City",
  "bin": {
    "prefab-tools": "./bin/run"
  },
  "homepage": "https://github.com/axe312ger/7dtd-prefab-tools",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "axe312ger/7dtd-prefab-tools",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^1.19.1",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.1.7",
    "archiver": "^5.3.1",
    "chalk": "4",
    "clone-deep": "^4.0.1",
    "expand-tilde": "^2.0.2",
    "fast-glob": "^3.2.12",
    "inquirer": "^8.2.5",
    "jimp": "^0.16.2",
    "memoizerific": "^1.11.3",
    "pretty-bytes": "^5",
    "three": "^0.146.0",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@oclif/test": "^2.2.10",
    "@types/archiver": "^5.3.1",
    "@types/chai": "^4",
    "@types/clone-deep": "^4.0.1",
    "@types/expand-tilde": "^2.0.0",
    "@types/inquirer": "^8.2.5",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.18.11",
    "@types/three": "^0.146.0",
    "@types/xml2js": "^0.4.11",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "mocha": "^9",
    "mocha-chai-jest-snapshot": "^1.1.4",
    "oclif": "^3",
    "shx": "^0.3.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.3"
  },
  "oclif": {
    "bin": "prefab-tools",
    "dirname": "prefab-tools",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "ts-node test/prepare-test-config.ts && mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/axe312ger/7dtd-prefab-tools/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}